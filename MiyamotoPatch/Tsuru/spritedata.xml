<?xml version="1.0"?>

<!-- Miyamoto Spritedata Patch for Tsuru -->

<sprites>
    <sprite id="724" name="Actor Spawner">
        <value nybble="1-2" title="Triggering Event ID"/>
        <list nybble="21" title="ID Type">
            <entry value="0">Profile ID</entry>
            <entry value="1">Sprite ID</entry>
        </list>
        <value nybble="22-24" title="ID"/>
        <value nybble="3" title="Child Event ID 1 (Nybble 2)"/>
        <value nybble="4" title="Child Event ID 2 (Nybble 4)"/>
        <value nybble="5" title="Child Nybble 5"/>
        <value nybble="6" title="Child Nybble 6"/>
        <value nybble="7" title="Child Nybble 7"/>
        <value nybble="8" title="Child Nybble 8"/>
        <value nybble="9" title="Child Nybble 9"/>
        <value nybble="10" title="Child Nybble 10"/>
        <value nybble="11" title="Child Nybble 11"/>
        <value nybble="12" title="Child Nybble 12"/>
        <value nybble="13" title="Child Nybble 13"/>
        <value nybble="14" title="Child Nybble 14"/>
        <value nybble="15" title="Child Nybble 15"/>
        <value nybble="16" title="Child Nybble 16"/>
        <value nybble="17" title="Child Nybble 17"/>
        <value nybble="18" title="Child Nybble 18"/>
        <value nybble="19" title="Child Nybble 19"/>
        <value nybble="20" title="Child Nybble 20"/>
    </sprite>
    <sprite id="725" name="Para-Bones"></sprite>
    <sprite id="726" name="Custom Door" notes="Inherits DoorBase nybbles.">
        <checkbox nybble="5" title="Z-Order" comment="Makes the sprite appear behind Layer 1"/>
        <checkbox nybble="6" title="Doesn't appear in challenges" comment="When checked, the door does not appear in challenges"/>
    </sprite>
    <sprite id="727" name="Fake Actor">
        <list nybble="12" title="Type">
            <entry value="0">Checkpoint</entry>
            <entry value="1">Goal Pole</entry>
            <entry value="2">Star Coin</entry>
        </list>
        <checkbox nybble="5" title="Debug Mode" comment="Prevents fake actor from despawning when triggered"/>
    </sprite>
    <sprite id="728" name="Custom Powerup"></sprite>
    <sprite id="729" name="Star Coin Shard">
        <list nybble="9" title="Star Coin ID" comment="Must be set on the shard which will spawn the coin (last collected)">
            <entry value="1">1st Star Coin</entry>
            <entry value="2">2nd Star Coin</entry>
            <entry value="3">3rd Star Coin</entry>
        </list>
    </sprite>
    <sprite id="730" name="Beep Block"></sprite>
    <sprite id="731" name="Falling Chestnut"></sprite>
    <sprite id="732" name="Task Changer" notes="! Unfinished"></sprite>
    <sprite id="733" name="Cataquack" notes="! Unfinished + Requires model"></sprite>
    <sprite id="734" name="Time Clock" notes="Adds or removes time from the timer when collected">
        <value nybble="10-12" title="Time Amount"/>
        <checkbox nybble="5" title="Remove Time" comment="Time Amount will be treated as a negative number"/>
    </sprite>
    <sprite id="735" name="Effect Spawner" notes="Spawns sound and particle effects"></sprite>
    <sprite id="736" name="Angry Sun"></sprite>
    <sprite id="737" name="Angry Moon"></sprite>
    <sprite id="738" name="Rainbow Light" notes="A configurable colorful light source"></sprite>
    <sprite id="739" name="Wave Platform" notes="Platform for water waves"></sprite>
    <sprite id="740" name="Debug Actor" notes="Actor useful for debugging various things"></sprite>
</sprites>
